var int a;
var int b;
var point p1;
var point p2;
var point p3;

class point {
  attribute int x;
  attribute int y;

  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }
}

main {
  a = 10;
  b = 11;
  p1 = new point(1, 2);
  p2 = new point(1, 2);
  p3 = new point(1, 3);

  //True, False
  print(24 === 24);
  print(11 === 1);
  
  // True, False, True
  print(a === a);
  print(a =/= a);
  print(a =/= b);

  //True, False, True, False
  print(p1 === p1);
  print(p1 =/= p1);
  print(p1 === p2);
  print(p1 === p3);
}
